{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": ["error", { "allowAfterThis": true }],
        "no-restricted-globals": "warn",
        "linebreak-style": 0,
        "consistent-return": 0,
        "no-prototype-builtins": 0
    },
    "plugins": ["jasmine"]
}
